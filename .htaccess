RewriteEngine On

RewriteRule ^([^/\.]+)/([^/\.]+)/([^/\.]+)/?$ index.php?object=$1&method=$2&id=$3 [L]
RewriteRule ^([^/\.]+)/([^/\.]+)/?$ index.php?object=$1&method=$2 [L]
RewriteRule ^([^/\.]+)/?$ index.php?object=$1 [L]

#RewriteRule ^([^/\.]+)/([^/\.]+)/([^/\.]+)/?$ index.php?page=$1&b=$2&c=$3 [L]
#RewriteRule ^([^/\.]+)/([^/\.]+)/?$ index.php?page=$1&b=$2 [L]
#RewriteRule ^([^/\.]+)/?$ index.php?page=$1 [L]